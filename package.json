{
  "name": "websvr",
  "version": "0.0.0",
  "private": true,
  "description": "A fine tune framework for angular-nodejs webserver",
  "repository": "https://github.com/leonkaihao/NgWebSvr",
  "scripts": {
    "start": "node app.js",
    "postinstall": "./node_modules/.bin/bower install",
    "prestart": "npm update",
    "test": "./node_modules/.bin/jshint app.js && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcov -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info && ./node_modules/.bin/gulp coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "body-parser": "^1.13.2",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "ejs": "^2.3.2",
    "excel-export": "^0.4.1",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "hiredis": "^0.3.0",
    "ioredis": "^1.5.12",
    "method-override": "^2.3.3",
    "mongoose": "^4.0.6",
    "morgan": "^1.6.1",
    "serve-favicon": "^2.3.0",
    "strip-json-comments": "^1.0.2",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-filter": "^2.0.2",
    "gulp-html-minifier": "^0.1.6",
    "gulp-jshint": "^1.11.2",
    "gulp-json-editor": "^2.2.1",
    "gulp-jsonlint": "^1.1.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-ngdocs": "^0.2.13",
    "gulp-rename": "^1.2.2",
    "gulp-strip-json-comments": "^1.0.2",
    "gulp-uglify": "^1.2.0",
    "istanbul": "^0.3.17",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "karma": "^0.13.3",
    "mocha": "^2.2.5",
    "protractor": "^2.1.0"
  }
}
